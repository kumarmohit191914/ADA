#include<iostream>
#include<cmath>
using namespace std;
void power(double m,int n=2)
{
	float answer;
	answer=pow(m,n);
	cout<<answer;
}
int main()
{
	double m;
	int n;
	cout<<"enter value of m:";
	cin>>m;
	cout<<"enter value of n:";
	cin>>n;
	if(n==0)
	{
			cout<<"the output is";
		power(m);
	}
	else
	{
			cout<<"the output is";
		power(m,n);
	}
}
